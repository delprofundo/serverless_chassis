{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-prototype-builtins": 0,
    "max-len": [1, 140, 2, {"ignoreComments": true}],
    "no-unused-vars": "warn",
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "prefer-const": 0,
    "space-in-parens": [ "error", "always" ],
    "indent": [ "error", 2]
  },
  "extends": [ "airbnb-base"]
}